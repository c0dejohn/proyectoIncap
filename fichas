     <SCRIPT language="JavaScript">
        contador=0;
        final=0;
        // script para datos personales
		var reg=new Array(53);

		function adicion(){
		    final=contador; 
			nombre=document.getElementById("nombre").value;
			identificacion=document.getElementById("identificacion").value;
			celular=document.getElementById("celular").value;
			celular2=document.getElementById("celular2").value;
			fn=document.getElementById("fn").value;
			est=document.getElementById("est").value;
			obs=document.getElementById("obs").value;
			direccion=document.getElementById("direccion").value;
			telefono=document.getElementById("telefono").value;
			correo=document.getElementById("correo").value;
			celular3=document.getElementById("celular3").value;
			//gen=document.getElementById("genero");
			//genero=gen.options[gen.SelectedIndex].text;
			genero=document.getElementById("genero").value;
			
			reg[contador] = new Array(53);
		    reg[contador][0]=nombre;
		    reg[contador][1]=identificacion;
		    reg[contador][2]=celular;
		    reg[contador][5]=celular2;
		    reg[contador][6]=fn;
		    reg[contador][3]=direccion;
		    reg[contador][4]=telefono;
		    reg[contador][7]=est;
		    reg[contador][8]=obs;
		    reg[contador][9]=correo;
		    reg[contador][10]=celular3;
		    reg[contador][11]=genero;
		  
		    alert("Registro Guardado..");
		    contador=contador + 1;
		    document.getElementById("nombre").value="";
			document.getElementById("identificacion").value="";
			document.getElementById("celular").value="";
			document.getElementById("celular2").value="";
			document.getElementById("fn").value="";
			document.getElementById("est").value="";
			document.getElementById("obs").value="";
			document.getElementById("direccion").value="";
			document.getElementById("telefono").value="";
			document.getElementById("correo").value="";
			document.getElementById("celular3").value="";
					    
		}
		function siguiente(){
		   if(contador < final ){
			 contador=contador + 1;
			} 
		   document.getElementById("nombre").value=reg[contador][0];
		   document.getElementById("identificacion").value=reg[contador][1];
		   document.getElementById("celular").value=reg[contador][2];
		   document.getElementById("celular2").value=reg[contador][5];
		   document.getElementById("fn").value=reg[contador][6];
		   document.getElementById("est").value=reg[contador][7];
		   document.getElementById("obs").value=reg[contador][8];
		   document.getElementById("direccion").value=reg[contador][3];
		   document.getElementById("telefono").value=reg[contador][4];
		   document.getElementById("correo").value=reg[contador][9];
		   document.getElementById("celular3").value=reg[contador][10];
		  
		   
		alert("siguiente" + contador);
		}
		function anterior(){
		   if(contador > 0){
		     contador=contador - 1;
		   }
		   document.getElementById("nombre").value=reg[contador][0];
		   document.getElementById("identificacion").value=reg[contador][1];
		   document.getElementById("celular").value=reg[contador][2];
		   document.getElementById("celular2").value=reg[contador][5];
		   document.getElementById("fn").value=reg[contador][6];
		   document.getElementById("est").value=reg[contador][7];
		   document.getElementById("obs").value=reg[contador][8];		   
		   document.getElementById("direccion").value=reg[contador][3];
		   document.getElementById("telefono").value=reg[contador][4];
		   document.getElementById("correo").value=reg[contador][9];
		   document.getElementById("celular3").value=reg[contador][10];
				   
		alert("anterior" + contador);
		}
  

