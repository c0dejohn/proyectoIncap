	//Datos nomina------------------------------------------------	
		function datosNomina(){
			final=contador;


			var tipoc=document.getElementById("tipoc").value;
			var fcontrato=document.getElementById("fcontrato").value;     	
			var ffcontrato=document.getElementById("ffcontrato").value;
			var departamento=document.getElementById("departamento").value;
			var cargo=document.getElementById("cargo").value;
			var estadoe=document.getElementById("estadoe").value;
			var sueldo=document.getElementById("sueldo").value;
			var funciones=document.getElementById("funciones").value;
			var emaili=document.getElementById("emaili").value;
			var historial=document.getElementById("historial").value;
			var obs1=document.getElementById("obs1").value;
			var jinmediato=document.getElementById("jinmediato").value;
			var eps=document.getElementById("eps").value;	
			var arp=document.getElementById("arp").value;
			var fonp=document.getElementById("fonp").value;
			var cconp=document.getElementById("cconp").value;


			reg[contador] = new Array(53);
			reg[contador][13]=tipoc;
			reg[contador][14]=fcontrato;
			reg[contador][15]=ffcontrato;
			reg[contador][16]=departamento;
			reg[contador][17]=cargo;
			reg[contador][18]=estadoe;
			reg[contador][19]=sueldo;
			reg[contador][44]=funciones;
			reg[contador][45]=emaili;
			reg[contador][46]=historial;
			reg[contador][12]=obs1;
			reg[contador][47]=jinmediato;
			reg[contador][48]=eps;
			reg[contador][49]=arp;
			reg[contador][50]=fonp;
			reg[contador][51]=cconp;
			alert("Registro almacenado...");
			contador=contador+1;

			document.getElementById("tipoc").value="";
			document.getElementById("fcontrato").value="";
			document.getElementById("ffcontrato").value="";
			document.getElementById("departamento").value="";
			document.getElementById("cargo").value="";
			document.getElementById("estadoe").value="";
			document.getElementById("sueldo").value="";
			document.getElementById("funciones").value="";
			document.getElementById("emaili").value="";
			document.getElementById("historial").value="";
			document.getElementById("obs1").value="";
			document.getElementById("jinmediato").value="";
			document.getElementById("eps").value="";
			document.getElementById("arp").value="";
			document.getElementById("fonp").value="";
			document.getElementById("cconp").value="";
		}
