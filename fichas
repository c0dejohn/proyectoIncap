//Nomina----------------------------------------------------

function cNomina(){
			
	final=contador;

	var sueldop=document.getElementById("sueldop").value;
	var htrab=document.getElementById("htrab").value;
	var hed=document.getElementById("hed").value;
	var hen=document.getElementById("hen").value;
	var hef=document.getElementById("hef").value;

	var vhn=document.getElementById("vhn").value=Math.round(sueldop/240);
	var thed=document.getElementById("thed").value=Math.round(hed*(vhn*1.25));
	var then=document.getElementById("then").value=Math.round(hen*(vhn*1.50));
	var thef=document.getElementById("thef").value=Math.round(hef*(vhn*1.75));
	var vthe=document.getElementById("vthe").value=thed+then+thef;

function subTransp(){
				if(vhn*htrab<1600000){
						var subt=(78400/240)*htrab;
					}else{
						var subt=0;
						}
					return subt;
				}

	var st=document.getElementById("st").value=Math.round(subTransp());
	

	
	var bon=document.getElementById("bon").value=Math.round(sueldop*0.03);
	var com=document.getElementById("com").value=Math.round(sueldop*0.018);
	var odev=document.getElementById("odev").value=0;
	var tdev=document.getElementById("tdev").value=Math.round(((vhn*htrab)+vthe + bon+ com+ odev));

	function cooperativa(){
				var coope = tdev*0.5;
				if (coope>80000){
				var coop2 =80000;
						}else{
					var coop2=coope;
					}
				return coop2;

				}
		var coop=document.getElementById("coop").value=Math.round(cooperativa());
		var salud= document.getElementById("salud").value=Math.round(tdev*0.04);

		var pension=document.getElementById("pension").value=Math.round(tdev*0.06);
	function retencion(){
				if(tdev>=1600000){
					var rete=tdev*0.6;
				}else{
				var rete=0;
				}
			return rete;
		}
		var ret=document.getElementById("ret").value=retencion();
		var cajacomp=document.getElementById("cajacomp").value=tdev*0.04;

		var prestamo=document.getElementById("prestamo").value;
		var cuota=document.getElementById("cuota").value;
		var oded=document.getElementById("oded").value=prestamo/cuota;

		var  tded=document.getElementById("tded").value=Math.round(ret+cajacomp+oded);
		var tneto=document.getElementById("tneto").value=Math.round(tdev-tded);
		
}function guardarCNomina(){
final=contador;

	
			
	

	var sueldop=document.getElementById("sueldop").value;
	var htrab=document.getElementById("htrab").value;
	var hed=document.getElementById("hed").value;
	var hen=document.getElementById("hen").value;
	var hef=document.getElementById("hef").value;

	var vhn=document.getElementById("vhn").value=Math.round(sueldop/240);
	var thed=document.getElementById("thed").value=Math.round(hed*(vhn*1.25));
	var then=document.getElementById("then").value=Math.round(hen*(vhn*1.50));
	var thef=document.getElementById("thef").value=Math.round(hef*(vhn*1.75));
	var vthe=document.getElementById("vthe").value=thed+then+thef;

function subTransp(){
				if(vhn*htrab<1600000){
						var subt=(78400/240)*htrab;
					}else{
						var subt=0;
						}
					return subt;
				}

	var st=document.getElementById("st").value=Math.round(subTransp());
	

	
	var bon=document.getElementById("bon").value=Math.round(sueldop*0.03);
	var com=document.getElementById("com").value=Math.round(sueldop*0.018);
	var odev=document.getElementById("odev").value=0;
	var tdev=document.getElementById("tdev").value=Math.round(((vhn*htrab)+vthe + bon+ com+ odev));

	function cooperativa(){
				var coope = tdev*0.5;
				if (coope>80000){
				var coop2 =80000;
						}else{
					var coop2=coope;
					}
				return coop2;

				}
		var coop=document.getElementById("coop").value=Math.round(cooperativa());
		var salud= document.getElementById("salud").value=Math.round(tdev*0.04);

		var pension=document.getElementById("pension").value=Math.round(tdev*0.06);
	function retencion(){
				if(tdev>=1600000){
					var rete=tdev*0.6;
				}else{
				var rete=0;
				}
			return rete;
		}
		var ret=document.getElementById("ret").value=retencion();
		var cajacomp=document.getElementById("cajacomp").value=tdev*0.04;

		var prestamo=document.getElementById("prestamo").value;
		var cuota=document.getElementById("cuota").value;
		var oded=document.getElementById("oded").value=prestamo/cuota;

		var  tded=document.getElementById("tded").value=Math.round(ret+cajacomp+oded);
		var tneto=document.getElementById("tneto").value=Math.round(tdev-tded);
reg[contador] = new Array(53);
		reg[contador][20]=sueldop;
		reg[contador][21]=htrab;
		reg[contador][22]=hed;
		reg[contador][23]=hen;
		reg[contador][24]=hef;
		reg[contador][25]=vhn;
		reg[contador][26]=thed;
		reg[contador][27]=then;
		reg[contador][28]=thef;
		reg[contador][29]=vthe;
		reg[contador][30]=st;
		reg[contador][31]=bon;
		reg[contador][32]=com;
		reg[contador][33]=odev;
		reg[contador][34]=tdev;
		reg[contador][35]=coop;
		reg[contador][36]=pension;
		reg[contador][37]=ret;
		reg[contador][38]=cajacomp;
		reg[contador][39]=prestamo;
		reg[contador][40]=cuota;
		reg[contador][41]=oded;
		reg[contador][42]=tded;
		reg[contador][43]=tneto
		


		alert("Registro almacenado...");
			contador=contador+1;

			document.getElementById("sueldop").value="";
			document.getElementById("htrab").value="";
			document.getElementById("hed").value="";
			document.getElementById("hen").value="";
			document.getElementById("hef").value="";
			document.getElementById("vhn").value="";
			document.getElementById("thed").value="";
			document.getElementById("then").value="";
			document.getElementById("thef").value="";
			document.getElementById("vthe").value="";
			document.getElementById("st").value="";
			document.getElementById("bon").value="";
			document.getElementById("com").value="";
			document.getElementById("odev").value="";
			document.getElementById("tdev").value="";
			document.getElementById("coop").value="";
			document.getElementById("pension").value="";
			document.getElementById("ret").value="";
			document.getElementById("cajacomp").value="";
			document.getElementById("prestamo").value="";
			document.getElementById("cuota").value="";
